<?xml version="1.0" encoding="UTF-8"?>
<project>
    <!-- the version of maven's project object model -->
    <pomVersion>3</pomVersion>
    <!-- a unique name for this project -->
    <artifactId>cvsgrab</artifactId>
    <groupId>cvsgrab</groupId>
    <id>cvsgrab</id>
    <!-- a short but descriptive name for the project -->
    <name>CVSGrab</name>
    <!-- The version of the project under development, e.g.
       1.1, 1.2, 2.0-SNAPSHOT -->
    <currentVersion>2.2.3-SNAPSHOT</currentVersion>
    <!-- details about the organization that 'owns' the project -->
    <organization>
        <name>Ludovic Claude</name>
        <url>http://cvsgrab.sourceforge.net/</url>
        <logo>http://sourceforge.net/sflogo.php?group_id=52481&amp;type=5</logo>
    </organization>
    <!-- the year the project started -->
    <inceptionYear>2002</inceptionYear>
    <package>net.sourceforge.cvsgrab</package>
    <logo>http://cvsgrab.sourceforge.net/cvsgrab.png</logo>
    <description>CVSGrab is a CVS client that can be used then the firewall prevents standard CVS clients from working.
It uses the web interface for a CVS repository to bypass firewalls that block the 2401 port used by CVS, and a screen scrapping technology to localise and download the files from this web interface.</description>
    <!-- a short description of what the project does -->
    <shortDescription>CVS client that works behind firewalls.</shortDescription>
    <!-- the project home page -->
    <url>http://cvsgrab.sourceforge.net/</url>
    <issueTrackingUrl>https://sourceforge.net/tracker/?group_id=52481&amp;atid=467004</issueTrackingUrl>
    <siteAddress>cvsgrab.sourceforge.net</siteAddress>
    <siteDirectory>/home/groups/c/cv/cvsgrab/htdocs/</siteDirectory>
    <distributionSite>upload.sourceforge.net</distributionSite>
    <distributionDirectory>/incoming/</distributionDirectory>
    <!-- the version control repository and http url for online access
       the connection element has the form:
       scm:<system>:<system specific connection string> -->
    <repository>
        <connection>scm:cvs:pserver:anonymous@cvsgrab.cvs.sourceforge.net:/cvsroot/cvsgrab:cvsgrab</connection>
        <url>http://cvsgrab.cvs.sourceforge.net/cvsgrab/cvsgrab/</url>
    </repository>
    <!-- any mailing lists for the project -->
    <licenses>
